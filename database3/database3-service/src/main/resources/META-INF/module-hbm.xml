<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="site.hadef.database3.model.Gender" />
	<class name="site.hadef.database3.model.impl.GenderImpl" table="gender">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" column="id" name="genderId" type="long">
			<generator class="assigned" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" column="name" name="genderName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>